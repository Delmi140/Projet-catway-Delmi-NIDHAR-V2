<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>Liste des catways</title>
</head>
<body>
	<div>
        <h1>Liste des catways</h1>
	</div>
	<div>
        <table style="border: 1px solid black" >
            <thead>
                <tr>
                    <th style="border: 1px solid black">Numéro de pont</th>
                    <th style="border: 1px solid black">Type</th>
                    <th style="border: 1px solid black">Description de l’état du catway</th>
                </tr>
            </thead>
            <tbody>
                <% catways.forEach(element => { %>
                    <tr>
                        <td><%= element.catwayNumber %></td>
                        <td><%= element.type %></td>
                        <td><%= element.catwayState %></td>
                    </tr>
                    
                <% }) %>

            </tbody>
        </table>
	
	</div>
	    
	</body>
</html>
